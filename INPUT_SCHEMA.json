{
  "title": "Video Content Analytics & Engagement Intelligence Configuration",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "startUrls": {
      "title": "Start URLs",
      "type": "array",
      "description": "URLs to extract video-analytics data from",
      "editor": "requestListSources",
      "default": [
        {
          "url": "https://example.com"
        }
      ]
    },
    "maxItems": {
      "title": "Maximum Items",
      "type": "integer",
      "description": "Maximum number of items to extract",
      "default": 1000,
      "minimum": 1,
      "maximum": 10000
    },
    "maxConcurrency": {
      "title": "Concurrency",
      "type": "integer",
      "description": "Number of parallel requests",
      "default": 5,
      "minimum": 1,
      "maximum": 20
    },
    "outputFormat": {
      "title": "Output Detail Level",
      "type": "string",
      "description": "Choose the level of detail in extracted data",
      "enum": [
        "basic",
        "detailed",
        "comprehensive"
      ],
      "default": "comprehensive"
    },
    "enableAnalytics": {
      "title": "Enable Advanced Analytics",
      "type": "boolean",
      "description": "Include advanced analytics and insights",
      "default": true
    },
    "dataQualityCheck": {
      "title": "Data Quality Validation",
      "type": "boolean",
      "description": "Perform data quality checks and scoring",
      "default": true
    },
    "proxyConfiguration": {
      "title": "Proxy Configuration",
      "type": "object",
      "description": "Proxy settings for reliable scraping",
      "editor": "proxy",
      "default": {
        "useApifyProxy": true
      }
    }
  },
  "required": [
    "startUrls"
  ]
}